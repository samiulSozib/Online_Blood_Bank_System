<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinebloodbanksystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DeactiveAccount"></activity>
        <activity android:name=".AddDonorActivity" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".Profile" />
        <activity android:name=".MakeRequest" />
        <activity android:name=".AllRequest" />
        <activity android:name=".Donor" />
        <activity android:name=".AllUser" />
        <activity android:name=".Home" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VerifyAccount" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>